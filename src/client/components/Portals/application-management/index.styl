#applications-management-layout {
  display: flex;
  margin: 20px 0;
}
#applications-management-layout .muuri-grid {
  position: relative;
  width: 100%;
  border: 5px solid transparent;
}
#applications-management-layout .muuri-grid-item {
  position: absolute;
  z-index: 1;
  width: 273px;
  height: 194px;
}
#applications-management-layout .muuri-grid-item.muuri-item-hidden {
  z-index: 0;
}
#applications-management-layout .muuri-grid-item.muuri-item-releasing {
  z-index: 2;
}
#applications-management-layout .muuri-grid-item.muuri-item-dragging {
  z-index: 3;
}
#applications-management-layout .muuri-grid-item-content {
  position: relative;

  color #000
  text-align center
  background-color: #fff
  width:228px
  height:170px
  margin-right:20px
  margin-bottom:20px
  cursor pointer
}
#applications-management-layout .muuri-grid .muuri-grid-item-content {
  background: #fff;
}
#applications-management-layout .muuri-grid-item.muuri-item-dragging .muuri-grid-item-content,
#applications-management-layout .muuri-grid-item.muuri-item-releasing .muuri-grid-item-content {
  background: #ccc;
}

#applications-management-layout 
  .application-manager-grid-item-top
      height: 130px;
      border-radius: 6px;
      overflow: hidden;
      .application-manager-grid-item-top-img
        width: 100%
        height: 130px
        pointerEvents: none
  .application-manager-grid-item-bottom
      line-height: 62px;
      height: 40px;
      lineHeight: 62px
      text-align left
