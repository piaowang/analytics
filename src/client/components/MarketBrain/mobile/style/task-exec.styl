$active-active-background-path = "../../../../../../node_modules/sugo-analytics-static/assets/images/market-brain-h5/header2.png"
$active-active-filter-icon = "../../../../../../node_modules/sugo-analytics-static/assets/images/market-brain-h5/filter-icon.png"
$active-active-selected-cancle-icon = "../../../../../../node_modules/sugo-analytics-static/assets/images/market-brain-h5/selected-cancle-icon.png"
#container
  height 100%
select{
-webkit-appearance:none;appearance:none;
}
#market-brain
  height 100%
  width 100%
  //活动详情
  #market-brain-task-exec
    height 100%
    background-color #FFF2C9
    .task-exec-drawer-filterbox
      .filter-name
        padding-left .1rem
        margin-top 44px
        .filter-name-title
          font-size 12px
          color #595558
          margin-bottom 10px
        .filter-name-input input
          border 4px solid #F0F0F0
          font-size 12px
          width 235px
          height 30px
          line-height 30px
          color #C5C5C5
          text-indent 4px
        .filter-name-input input::-webkit-input-placeholder
          color:#C5C5C5;
        .filter-name-input input::-moz-placeholder 
          color:#C5C5C5;
        .filter-name-input input:-moz-placeholder
          color:#C5C5C5;
        .filter-name-input input:-ms-input-placeholder
          color:#C5C5C5;
      .filter-active
        padding-left .1rem
        margin-top 26px
        padding-bottom 10px
        border-bottom 1px solid #F8F4F8
        .filter-active-title
          font-size 12px
          color #595558
          margin-bottom 10px
        .filter-active-select
          border 4px solid #F0F0F0
          font-size 12px
          width 235px
          height 30px
          line-height 26px
          color #C5C5C5
          text-indent 4px
      .filter-time
        padding-left .1rem
        margin-top 12px
        padding-bottom 10px
        border-bottom 1px solid #F8F4F8
        .filter-time-title
          font-size 12px
          color #595558
          margin-bottom 10px
        .filter-time-input
          border 4px solid #F0F0F0
          font-size 12px
          width 235px
          height 30px
          line-height 26px
          color #C5C5C5
          text-indent 4px
          // background-color #F0F0F0
          // width 2.55rem
          // height 32px
          // line-height 26px
          // padding 0 5px
          // span:nth-child(odd)
          //   background #ffffff
          //   display inline-block
          //   width .65rem
          //   height 24px
          //   text-align center
          //   color #C5C5C5
          // span:nth-child(even)
          //   font-size 24px
          //   color #A5A5A7
          //   margin 0 5px
      .filter-state
        padding-left .1rem
        margin-top 12px
        padding-bottom 10px
        border-bottom 1px solid #F8F4F8
        .filter-state-title
          font-size 12px
          color #595558
          margin-bottom 10px
        .filter-state-checkbox
          span 
            display inline-block
            margin-right 6px
            width 78px
            height 30px
            line-height 32px
            font-size 12px
            background-color #F0EFF2
            color #211D20
            border-radius 4px
            text-align center
          .cur
            background-color #FFF2C9
            color #FFA608
            position relative
          .cur::before
            content: ''
            display inline-block
            position absolute
            width 18px
            height 18px
            background-image url($active-active-selected-cancle-icon)
            background-repeat no-repeat
            background-size contain
            right 0
            bottom 0
      .drawer-footer
        width 100%
        text-align center
        color #ffffff
        font-size 16px
        position absolute
        bottom 0
        .cancel
          display inline-block
          background-color #FFAE07
          width 50%
          height .4rem
          line-height .4rem
        .finish
          display inline-block
          background-color #FF5158
          width 50%
          height .4rem
          line-height .4rem
    .header
      position relative
      width 100%
      height 1.4rem
      background-image url($active-active-background-path)
      background-repeat no-repeat
      background-size contain
      padding-top .6rem
      text-align center
      font-size 22px
      color #FFFFFF
      font-weight 999
    .filter-icon
      width .3rem
      height .3rem
      z-index 999
      background-image url($active-active-filter-icon)
      background-repeat no-repeat
      background-size contain
      position absolute
      right .2rem
      top .35rem
    .body
      height calc(100vh - 1.4rem - 0.65rem)
      overflow scroll
      .task-exec-item
        margin .1rem auto
        width 3.35rem
        background #ffffff
        padding 16px
        .task-exec-item-header
          width 3.05rem
          height 20px
          line-height 20px
          margin 0 auto
          border-bottom 1px solid #FFF2C9
          position relative
          .username
            position absolute
            font-size 16px
            color #202020
          .mobile
            position absolute
            right 0
            font-size 12px
            color #202020
        .task-exec-item-body
          margin-top 12px
          .body-left
            display inline-block
            width 1.67rem
            .active-name
              color #202020
              font-size 12px
            .active-exec
              color #999999
              font-size 12px
          .body-right
            display inline-block
            vertical-align super
            width .7rem
            margin-left .6rem
            .unexec
              text-align center
              background-color #FFAE07
              color #ffffff
              height 26px
              line-height 28px
              width 70px
              border-radius 4px
            .finished
              text-align center
              background-color #ffffff
              color #CCCCCC
              border 1px solid #ccc
              height 26px
              line-height 28px
              width 70px
              border-radius 4px