
.web-heatmap-wrapper
  .ant-switch
    background-color #888
  .ant-switch-checked
    background-color #1890ff
  .top-toolbar1
    background #4C4C52
    padding 10px 16px 10px 20px
    color #fff
    
  .top-toolbar2
    padding 10px 16px 10px 20px
    box-shadow 0px 3px 10px 0px rgba(235, 235, 235, 1)
    border-bottom 1px solid #ddd

  .view-btn
    line-height 38px
    height 38px
  .main-content-wrapper
    height calc(100% - 58px)
  .hm-frame-container
    margin 0 auto
    iframe
      width 100%
      height 100%
      border none
  .tabs-group-button
    margin-bottom -6px
    .tabs-button
      height 43px
      line-height 43px
      border 1px solid #eee
      border-radius 8px 8px 0px 0px
      width 120px
      text-align center
    .button-checked
      font-weight bold
      color #fff
      background-color #467EDB
    
