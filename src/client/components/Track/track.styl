.sdk-btn-shadow
  position: absolute
  cursor: pointer

.sdk-btn-shadow:hover , .sdk-btn-shadow-click
  border:3px solid #4ba8ff
  border-radius:3px
  background:rgba(83,163,235,.2)
  box-sizing:border-box
  box-shadow:0 12px 15px 0 rgba(0,0,0,.24),0 17px 50px 0 rgba(0,0,0,.19)

.sdk-btn-shadow-bind
  border:3px solid #1bb39c
  border-radius:3px
  background:rgba(13,139,113,.2)
  box-sizing:border-box
  box-shadow:0 12px 15px 0 rgba(0,0,0,.24),0 17px 50px 0 rgba(0,0,0,.19)

.sdk-btn-shadow-dim-bind
  border:3px solid #9270e2
  border-radius:3px
  background:rgba(146,112,226,.2)
  box-sizing:border-box
  box-shadow:0 12px 15px 0 rgba(0,0,0,.24),0 17px 50px 0 rgba(0,0,0,.19)

